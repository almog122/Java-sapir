package lab9;
import java.util.Scanner;
public class zibi2 {
	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		
		int a=0, b=0;
		System.out.println("enter a positive number:");
		int d=input.nextInt();
		while (d!=0) {
			a=a+1;
			b=b+(d%10);
			d=d/10;
		}
		if(b%a==0)
			System.out.println("Yes.");
		else 
			System.out.println("No.");
		input.close();
	}
}
